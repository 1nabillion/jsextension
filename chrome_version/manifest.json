{
  "manifest_version": 3,
  "name": "JS Endpoint Extractor",
  "version": "1.0",
  "description": "Extracts endpoints from JS files and stores them.",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}